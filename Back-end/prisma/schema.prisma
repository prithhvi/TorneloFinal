generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model Product {
  id          Int       @id @default(autoincrement()) @db.SmallInt
  createdAt   DateTime? @default(now()) @db.Timestamptz(6)
  prodName    String?   @db.VarChar
  prodDesc    String?   @map(" prodDesc") @db.VarChar
  prodCost    Int?
  prodImg     String[]  @db.VarChar
  prodVariant Json?     @db.Json
  orgId       Int?
  stockCount  Int?
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model ShoppingCart {
  id           Int       @id @default(autoincrement())
  createdAt    DateTime? @default(now()) @db.Timestamptz(6)
  userId       Int?      @db.SmallInt
  prodQuantity Int?
  prodCost     Int?
  prodId       Int?      @db.SmallInt
  prodName     String?   @db.VarChar
  prodImg      String[]  @db.VarChar
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model Analytics {
  id         Int       @id @default(autoincrement())
  name       String
  amount     Int?
  totalSales Int?
  views      Int?
  uptakes    Int?
  month      DateTime? @db.Date
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model User {
  createdAt     DateTime? @default(now()) @db.Timestamptz(6)
  userId        Int       @id @unique(map: "User_user_id_key")
  userId        Int       @id @unique(map: "User_user_id_key")
  userEmail     String?
  userAddress   String?
  userPhoneNo   String?
  userFirstName String?
  userLastName  String?
  userrole      String?   @default("User")
  userPassword  String
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model CompletedOrders {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  userId       Int?
  prodQuantity Int?
  prodCost     Int?
  prodId       Int?
  prodName     String?  @db.VarChar
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model ShippingInformation {
  shippingId Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  name       String?  @db.VarChar
  address    String?  @db.VarChar
  email      Json?    @db.Json
  phone      String?  @db.VarChar
  postCode   String?  @db.VarChar
  userId     Int?
  state      String?  @db.VarChar
}
